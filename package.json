{
  "name": "cloudinsight",
  "version": "0.0.3",
  "description": "SDK for Cloud Insight",
  "main": "dist/index.js",
  "scripts": {
    "ci":"babel-node node_modules/.bin/isparta cover _mocha -- tests/**/*.test.js --compilers js:babel-core/register --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cov": "babel-node node_modules/.bin/isparta cover _mocha -- tests/**/*.test.js --compilers js:babel-core/register",
    "test": "mocha tests/**/*.test.js --compilers js:babel-core/register",
    "compile": "babel lib --no-comments --minified --copy-files --source-maps=inline --presets es2015 --out-dir=dist",
    "help": "babel -h",
    "release-patch": "npm run-scrip compile && npm version patch && npm publish && curl -H 'Content-Length:0' -X PUT https://npm.taobao.org/sync/cloudinsight"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wyvernnot/node-cloudinsight-sdk.git"
  },
  "author": "wyvernnot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wyvernnot/node-cloudinsight-sdk/issues"
  },
  "homepage": "https://github.com/wyvernnot/node-cloudinsight-sdk#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "coveralls": "^2.11.6",
    "express": "^4.13.4",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "oneapm-sdk": "0.0.5",
    "webpack": "^1.12.13",
    "webpack-dev-middleware": "^1.5.1"
  }
}
